    "@faker-js/faker": "8.4.1",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.12.12",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "1.6.0",
    "prisma": "5.14.0",
    "reflect-metadata": "0.2.2",
    "supertest": "7.0.0",
    "tsup": "8.0.2",
    "tsx": "4.10.5",
    "tsyringe": "4.8.0",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "1.6.0"
  },
